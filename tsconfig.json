{
    "compilerOptions": {
        "jsx": "react-native",
        "target": "esnext",
        "lib": ["dom", "esnext"],
        "skipLibCheck": true,
        "noEmit": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "strict": true,
        "downlevelIteration": true,
        "module": "commonjs",
        "baseUrl": ".",
        "paths": {
            "*": ["./src/*"],
            "config": ["./config"]
        }
    },
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js"
    ]
}
